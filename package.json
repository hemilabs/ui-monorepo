{
  "name": "ui-monorepo",
  "version": "1.0.0",
  "scripts": {
    "deps:check": "knip",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "postinstall": "patch-package",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "better-sort-package-json": "1.1.1",
    "commitlint-config-bloq": "1.1.0",
    "eslint": "8.57.0",
    "eslint-config-bloq": "4.7.0",
    "husky": "9.1.7",
    "knip": "5.61.3",
    "lint-staged": "15.2.10",
    "patch-package": "8.0.0",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.9"
  },
  "private": true,
  "workspaces": [
    "packages/*",
    "portal",
    "portal-backend/api",
    "portal-backend/cron/*",
    "subgraph-api",
    "subgraphs/*"
  ]
}
